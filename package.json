    "url": "https://github.com/noook/fat-track/issues"
  },
  "homepage": "https://github.com/noook/fat-track#readme",
  "dependencies": {
    "faunadb": "^2.14.1",
    "netlify-lambda": "^1.6.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/node": "^14.0.5",
    "netlify-cli": "^2.51.0",
    "typescript": "^3.9.3"
